import java.util.*;
public class MyClass {
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);
        int num =sc.nextInt();
        int a[] = new int[num];
        int sum=0;
        for(int i=0;i<num;i++){
            a[i]=sc.nextInt();
            sum+=a[i];
        }
        System.out.println(sum);
    }
}
